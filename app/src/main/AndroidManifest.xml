        </activity>
        <activity
            android:name=".SampleResolution"
            android:label="@string/title_activity_sample_resolution"
            android:parentActivityName=".WordzBasic" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.yogesh.wordzapp.WordzBasic" />
        </activity>
        <activity
            android:name=".PointsAndMotionUsage"
            android:label="@string/title_activity_points_and_motion_usage"
            android:parentActivityName=".WordzBasic"
            android:screenOrientation="landscape">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.yogesh.wordzapp.WordzBasic" />
        </activity>
    </application>

</manifest>
